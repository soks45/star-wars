{
    "name": "star-wars",
    "version": "0.0.0",
    "scripts": {
        "prepare": "husky",
        "ng": "ng",
        "start": "ng serve --configuration development",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "__lint__": "",
        "lint": "eslint \"**/*.{js,ts,html}\" --quiet --fix",
        "format": "prettier \"**/*.{js,ts,html,css,scss,json}\" --write",
        "lint-format": "npm run format && npm run lint",
        "commitlint": "commitlint --edit"
    },
    "lint-staged": {
        "*.{ts,js,html}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.scss": [
            "prettier --write"
        ]
    },
    "private": true,
    "packageManager": "npm@10.8.2",
    "dependencies": {
        "@angular/cdk": "21.0.0",
        "@angular/common": "21.0.0",
        "@angular/compiler": "21.0.0",
        "@angular/core": "21.0.0",
        "@angular/forms": "21.0.0",
        "@angular/platform-browser": "21.0.0",
        "@angular/router": "21.0.0",
        "@taiga-ui/addon-table": "^4.61.0",
        "@taiga-ui/cdk": "4.61.0",
        "@taiga-ui/core": "4.61.0",
        "@taiga-ui/event-plugins": "4.7.0",
        "@taiga-ui/icons": "4.61.0",
        "@taiga-ui/kit": "4.61.0",
        "@taiga-ui/layout": "4.61.0",
        "rxjs": "7.8.0",
        "tslib": "2.3.0"
    },
    "devDependencies": {
        "@angular-eslint/eslint-plugin": "20.6.0",
        "@angular-eslint/eslint-plugin-template": "20.6.0",
        "@angular-eslint/schematics": "20.6.0",
        "@angular-eslint/template-parser": "20.6.0",
        "@angular/build": "21.0.0",
        "@angular/cli": "21.0.0",
        "@angular/compiler-cli": "21.0.0",
        "@commitlint/cli": "20.1.0",
        "@commitlint/config-conventional": "20.0.0",
        "@typescript-eslint/eslint-plugin": "8.47.0",
        "@typescript-eslint/parser": "8.47.0",
        "eslint": "8.57.1",
        "eslint-config-prettier": "10.1.8",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-boundaries": "5.2.0",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-unused-imports": "4.3.0",
        "husky": "9.1.7",
        "less": "4.4.2",
        "lint-staged": "16.2.7",
        "typescript": "5.9.2"
    }
}
